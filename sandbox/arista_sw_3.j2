interface Ethernet{{ intf_num }}
!
{% endfor %}
interface Management1
   shutdown
!
interface Vlan1
   ip address {{ mgmt_ip_address }}/24
!
ip route 0.0.0.0/0 {{ default_gateway }}
!
ip routing
!
management api http-commands
   no shutdown
!
!
